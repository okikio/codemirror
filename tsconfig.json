{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "jsx": "preserve",
        "jsxImportSource": "solid-js",
        "types": [
            "vite/client"
        ],
        "moduleResolution": "node",
        "target": "ES2021",
        "module": "ES2022",
        "lib": [
            "ES2021",
            "ES2020",
            "DOM",
            "WebWorker"
        ],
        "sourceMap": false,
        "esModuleInterop": true,
        "resolveJsonModule": true
    },
    "exclude": [
        "node_modules"
    ]
}